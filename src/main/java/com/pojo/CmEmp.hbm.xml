<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pojo.CmEmp" table="cm_emp" schema="career">
        <property name="etime">
            <column name="etime" sql-type="datetime"/>
        </property>
        <property name="esalary">
            <column name="esalary" sql-type="int(5)"/>
        </property>
        <property name="einfo">
            <column name="einfo" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="estate">
            <column name="estate" sql-type="int(5)"/>
        </property>
        <property name="ewq">
            <column name="ewq" sql-type="bit(1)" not-null="true"/>
        </property>
        <property name="eleave">
            <column name="eleave" sql-type="date" not-null="true"/>
        </property>
        <property name="ereason">
            <column name="ereason" sql-type="varchar(255)" not-null="true"/>
        </property>
        <many-to-one name="cmJobByJid" class="com.pojo.CmJob">
            <column name="jid"/>
        </many-to-one>
        <many-to-one name="cmStudentBySid" class="com.pojo.CmStudent">
            <column name="sid"/>
        </many-to-one>
        <many-to-one name="cmUserByUid" class="com.pojo.CmUser">
            <column name="uid"/>
        </many-to-one>
    </class>
</hibernate-mapping>