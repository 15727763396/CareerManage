<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pojo.CmUnemp" table="cm_unemp" schema="career">
        <property name="uesalary">
            <column name="uesalary" sql-type="int(5)" not-null="true"/>
        </property>
        <property name="uetime">
            <column name="uetime" sql-type="date" not-null="true"/>
        </property>
        <property name="ueschool">
            <column name="ueschool" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="uemajor">
            <column name="uemajor" sql-type="varchar(50)" length="50" not-null="true"/>
        </property>
        <property name="uesuccess">
            <column name="uesuccess" sql-type="int(5)" not-null="true"/>
        </property>
        <property name="uestate">
            <column name="uestate" sql-type="int(5)"/>
        </property>
        <many-to-one name="cmDirectionByDid" class="com.pojo.CmDirection">
            <column name="did"/>
        </many-to-one>
        <many-to-one name="cmJobByJid" class="com.pojo.CmJob">
            <column name="jid" not-null="true"/>
        </many-to-one>
        <many-to-one name="cmStudentBySid" class="com.pojo.CmStudent">
            <column name="sid"/>
        </many-to-one>
    </class>
</hibernate-mapping>