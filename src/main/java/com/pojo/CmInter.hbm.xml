<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pojo.CmInter" table="cm_inter" schema="career">
        <property name="iaddress">
            <column name="iaddress" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="itype">
            <column name="itype" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="itime">
            <column name="itime" sql-type="datetime"/>
        </property>
        <property name="isuccess">
            <column name="isuccess" sql-type="int(5)"/>
        </property>
        <property name="istate">
            <column name="istate" sql-type="int(5)"/>
        </property>
        <many-to-one name="cmAreaByAid" class="com.pojo.CmArea">
            <column name="aid"/>
        </many-to-one>
        <many-to-one name="cmRecruitByRid" class="com.pojo.CmRecruit">
            <column name="rid"/>
        </many-to-one>
        <many-to-one name="cmStudentBySid" class="com.pojo.CmStudent">
            <column name="sid"/>
        </many-to-one>
    </class>
</hibernate-mapping>