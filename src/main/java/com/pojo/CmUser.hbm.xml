<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pojo.CmUser" table="cm_user" schema="career">
        <property name="uname">
            <column name="uname" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="urname">
            <column name="urname" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="upwd">
            <column name="upwd" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="uemail">
            <column name="uemail" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="uphone">
            <column name="uphone" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="urank">
            <column name="urank" sql-type="int(5)"/>
        </property>
        <property name="ustate">
            <column name="ustate" sql-type="int(5)"/>
        </property>
        <set name="cmEmpsByUid" inverse="true">
            <key>
                <column name="uid"/>
            </key>
            <one-to-many not-found="ignore" class="com.pojo.CmEmp"/>
        </set>
    </class>
</hibernate-mapping>