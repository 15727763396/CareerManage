<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pojo.CmStudent" table="cm_student" schema="career">
        <property name="sno">
            <column name="sno" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="sname">
            <column name="sname" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="ssex">
            <column name="ssex" sql-type="bit(1)" not-null="true"/>
        </property>
        <property name="sbirth">
            <column name="sbirth" sql-type="date"/>
        </property>
        <property name="spro">
            <column name="spro" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="sgrade">
            <column name="sgrade" sql-type="int(5)"/>
        </property>
        <property name="sclass">
            <column name="sclass" sql-type="int(5)"/>
        </property>
        <property name="sphone">
            <column name="sphone" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="semail">
            <column name="semail" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="scode">
            <column name="scode" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="smark">
            <column name="smark" sql-type="int(5)" not-null="true"/>
        </property>
        <property name="sassess">
            <column name="sassess" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="sstate">
            <column name="sstate" sql-type="int(5)"/>
        </property>
        <property name="sdetail">
            <column name="sdetail" sql-type="varchar(255)" not-null="true"/>
        </property>
        <set name="cmEmpsBySid" inverse="true">
            <key>
                <column name="sid"/>
            </key>
            <one-to-many not-found="ignore" class="com.pojo.CmEmp"/>
        </set>
        <set name="cmGradesBySid" inverse="true">
            <key>
                <column name="sid"/>
            </key>
            <one-to-many not-found="ignore" class="com.pojo.CmGrade"/>
        </set>
        <set name="cmIntersBySid" inverse="true">
            <key>
                <column name="sid"/>
            </key>
            <one-to-many not-found="ignore" class="com.pojo.CmInter"/>
        </set>
        <set name="cmUnempsBySid" inverse="true">
            <key>
                <column name="sid"/>
            </key>
            <one-to-many not-found="ignore" class="com.pojo.CmUnemp"/>
        </set>
    </class>
</hibernate-mapping>