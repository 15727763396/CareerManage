<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.pojo.CmDirection" table="cm_direction" schema="career">
        <property name="dname">
            <column name="dname" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="dstate">
            <column name="dstate" sql-type="int(5)"/>
        </property>
        <set name="cmUnempsByDid" inverse="true">
            <key>
                <column name="did"/>
            </key>
            <one-to-many not-found="ignore" class="com.pojo.CmUnemp"/>
        </set>
    </class>
</hibernate-mapping>